{
    "manifest_version": 3,
    "name": "Card Tab - 我的导航",
    "version": "1.0.7",
    "description": "自定义新标签页导航，支持分类管理、拖拽排序、深色模式",
    "permissions": [
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "icons": {
        "16": "icons/icon128.png",
        "48": "icons/icon128.png",
        "128": "icons/icon128.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "cardtab@example.com",
            "strict_min_version": "109.0",
            "data_collection_permissions": {
                "required": [
                    "none"
                ]
            }
        }
    }
}